dram_mr1        = 0x40
dram_mr2        = 0x10
dram_mr3        = 0
dram_tpr0       = 0x04E214EA
dram_tpr1       = 0x004214AD
dram_tpr2       = 0x10A75030
dram_tpr3       = 0
dram_tpr4       = 0
dram_tpr5       = 0
dram_tpr6       = 0
dram_tpr7       = 0
dram_tpr8       = 0
dram_tpr9       = 0
dram_tpr10      = 0
dram_tpr11      = 0
